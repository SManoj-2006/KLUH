# Copy this file to .env — never commit .env to git
# cp .env.example .env

# Port the FastAPI pipeline will listen on
PIPELINE_PORT=8000

# Supabase connection (get from: Supabase Dashboard → Settings → API)
# Use the service_role (secret) key — NOT the anon/publishable key
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key
